{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/courses",
      "methods": ["POST", "PUT"],
      "dest": "app.js",
      "config": {
        "helpers": {
          "bodyParser": false
        }
      }
    },
    {
      "src": "/api/courses/(.*)/materials/(.*)/assignments",
      "methods": ["POST"],
      "dest": "app.js",
      "config": {
        "helpers": {
          "bodyParser": false
        }
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "app.js"
    }
  ]
}
